<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:background="#EBE8E3"
    tools:context=".GroceryStores">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="175dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="175dp"
        android:fontFamily="@font/kollektifbold"
        android:text="Grocery Stores Near You!"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="200dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/store1"
            android:layout_width="match_parent"
            android:layout_height="70sp"
            android:layout_marginTop="2sp"
            android:background="#DCD7D1"
            android:ems="10"
            android:fontFamily="@font/kollektif"
            android:inputType="text"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/store2"
            android:layout_width="match_parent"
            android:layout_height="70sp"
            android:layout_marginTop="2sp"
            android:background="#DCD7D1"
            android:ems="10"
            android:fontFamily="@font/kollektif"
            android:inputType="text"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/store3"
            android:layout_width="match_parent"
            android:layout_height="70sp"
            android:layout_marginTop="2sp"
            android:background="#DCD7D1"
            android:ems="10"
            android:fontFamily="@font/kollektif"
            android:inputType="text"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/store4"
            android:layout_width="match_parent"
            android:layout_height="70sp"
            android:layout_marginTop="2sp"
            android:background="#DCD7D1"
            android:ems="10"
            android:fontFamily="@font/kollektif"
            android:inputType="text"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/store5"
            android:layout_width="match_parent"
            android:layout_height="70sp"
            android:layout_marginTop="2sp"
            android:background="#DCD7D1"
            android:ems="10"
            android:fontFamily="@font/kollektif"
            android:inputType="text"
            android:textSize="18sp" />
    </LinearLayout>

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="179dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="184dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

</androidx.constraintlayout.widget.ConstraintLayout>